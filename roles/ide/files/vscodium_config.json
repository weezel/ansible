{
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingOvertype": "never",
    "editor.autoClosingQuotes": "never",
    "editor.autoClosingDelete": "never",
    "editor.cursorBlinking": "solid",
    "editor.fontFamily": "'Hack', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.insertSpaces": false,
    "editor.parameterHints.enabled": false,
    "editor.quickSuggestions": {
        "comments": "off",
        "strings": "off",
        "other": "off"
    },
    "editor.quickSuggestionsDelay": 99999,
    "editor.renderWhitespace": "trailing",
    "editor.suggestOnTriggerCharacters": false,
    "editor.tabSize": 8,
    "editor.rulers": [
        120
    ],
    "editor.suggest.insertMode": "insert",
    "explorer.confirmDragAndDrop": false,
    "go.formatTool": "gofumpt",
    "go.useLanguageServer": true,
    "go.lintTool": "golangci-lint",
    "go.lintFlags": [
        "--fast",
        "--fix"
    ],
    "go.delveConfig": {
        "showGlobalVariables": true,
    },
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "debug.openDebug": "neverOpen",
    "debug.internalConsoleOptions": "neverOpen",
    "debug.console.wordWrap": false,
    "telemetry.telemetryLevel": "off",
    "workbench.startupEditor": "none",
    "workbench.tree.indent": 22,
    "terminal.integrated.fontSize": 12,
    "html.autoClosingTags": false,
    "window.zoomLevel": 0.2,
    "todo-tree.general.statusBar": "top three",
    "code-eol.highlightNonDefault": true,
    "code-eol.highlightExtraWhitespace": true,
    "code-eol.decorateBeforeEol": true,
    "code-eol.newlineCharacter": " ",
    "workbench.enableExperiments": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.codeLens.enabled": false,
    "gitlens.defaultDateLocale": null,
    "gitlens.defaultTimeFormat": "HH:mm",
    "gitlens.defaultDateShortFormat": "DD-MM-YY",
    "gitlens.defaultDateFormat": "HH:MM DD-MM-YYYY",
    "gitlens.defaultDateStyle": "absolute",
    "gitlens.hovers.enabled": false,
    "editor.bracketPairColorization.enabled": false,
    "files.associations": {
        "*.gohtml": "html"
    },
    "outline.collapseItems": "alwaysCollapse",
    "sqltools.autoOpenSessionFiles": false,
    "sqltools.autoConnectTo": "",
    "sqltools.defaultExportType": "json",
    "sqltools.defaultOpenType": "json",
    "sqltools.disableReleaseNotifications": true,
    "editor.mouseWheelZoom": true,
    "go.editorContextMenuCommands": {
        "removeTags": true
    },
    "go.inlayHints.constantValues": true,
    "go.playground": {
        "openbrowser": false,
        "share": false,
        "run": false
    },
    "go.testExplorer.packageDisplayMode": "nested",
    "editor.stickyScroll.enabled": true,
    "files.enableTrash": false,
    "editor.accessibilitySupport": "off",
    "workbench.editor.closeEmptyGroups": false,
    "go.diagnostic.vulncheck": "Imports",
    "go.gopls": {
        "textDocument": {
            "codeAction": {
                "dataSupport": true,
                "resolveSupport": {
                    "properties": [
                        "edit"
                    ]
                }
            }
        }
    }
}